<?xml version="1.0" encoding="UTF-8"?>
<addon id="game.libretro.pcem" name="PCem" version="11.0.0" provider-name="Libretro">
    <requires>
        <import addon="game.libretro" version="1.0.0"/>
    </requires>
    <extension point="kodi.gameclient"
               library_linux="game.libretro.pcem.so">
        <platforms></platforms>
        <extensions>exe|cmd|bat</extensions>
        <supports_vfs>true</supports_vfs>
        <supports_standalone>true</supports_standalone>
        <supports_keyboard>true</supports_keyboard>
        <supports_mouse>true</supports_mouse>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary lang="en">PCem v11.0</summary>
      <description>PCem is a PC emulator specializes in running old operating systems and software that are designed for IBM PC compatibles.</description>
      <license>GPL v2.0</license>
      <platform>linux</platform>
      <nofanart>true</nofanart>
    </extension>
</addon>
